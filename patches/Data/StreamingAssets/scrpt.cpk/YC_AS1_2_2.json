    {
      "function": "XMESS",
      "argument": [
        "Well, in the summer there is a fire festival",
        "in Nanbu-cho, so you should go see it."
      ],
      "name": "Chiaki",
      "msgidx": 4,
      "voice": "CHI_0789_0003",
