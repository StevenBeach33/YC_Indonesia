      "name": "",
      "msgidx": -1,
      "voice": "",
      "vid": "",
      "value": "",
      "said": ""
    },
    {
      "function": "XMESS",
      "argument": [
        "Dari akhir Pertempuran Sekigahara hingga",
        "akhir zaman Edo, tempat ini terus makmur",
        "sebagai pusat klan Suwa, tapi..."
      ],
      "name": "Toba Minami",
      "msgidx": 5,
      "voice": "MIN_0551_0005",
      "vid": "VID_MIN",
      "value": "",
      "said": ""
    },
    {
      "function": "XWCR",
