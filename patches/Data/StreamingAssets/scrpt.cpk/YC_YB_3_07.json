      "name": "",
      "msgidx": -1,
      "voice": "",
      "vid": "",
      "value": "",
      "said": ""
    },
    {
      "function": "XMESS",
      "argument": [
        "Saat dibangun, kastil ini menghadap",
        "ke Danau Suwa sehingga disebut juga",
        "Kastil Terapung Suwa."
      ],
      "name": "Toba Minami",
      "msgidx": 4,
      "voice": "MIN_0550B_0004",
      "vid": "VID_MIN",
      "value": "",
      "said": ""
    },
    {
      "function": "XWCR",
