    {
      "function": "XMESS",
      "argument": [
        "Kastil Takashima adalah kastil air yang",
        "dibangun oleh Hineno Takayoshi,",
        "pengikut Toyotomi Hideyoshi."
      ],
      "name": "Toba Minami",
      "msgidx": 3,
      "voice": "MIN_0549_0003",
      "vid": "VID_MIN",
      "value": "",
      "said": ""
    },
    {
      "function": "XWCR",
